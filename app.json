{
  "expo": {
    "name": "napstr",
    "slug": "napstr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "napstr",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.karki0733.napstr",
      "appleTeamId": "U63RPYT5TG",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSMotionUsageDescription": "Allow Napstr to access your device motion for interactive UI effects"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.karki0733.napstr",
      "permissions": ["android.permission.HIGH_SAMPLING_RATE_SENSORS"]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      [
        "expo-sensors",
        {
          "motionPermission": "Allow Napstr to access your device motion for interactive UI effects"
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/grain_menu.png",
            "./assets/title.otf",
            "./assets/dosis_extra-light.ttf",
            "./assets/dosis_light.ttf",
            "./assets/dosis_book.ttf",
            "./assets/dosis_medium.ttf",
            "./assets/dosis_semi-bold.ttf",
            "./assets/dosis_bold.ttf",
            "./assets/dosis_extra-bold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    }
  }
}
