{
  "expo": {
    "name": "napstr",
    "slug": "napstr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "napstr",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.siddhant.napstr",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSMotionUsageDescription": "Allow Napstr to access your device motion for interactive UI effects"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.karki0733.napstr",
      "permissions": [
        "android.permission.HIGH_SAMPLING_RATE_SENSORS",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.WAKE_LOCK"
      ],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      [
        "expo-sensors",
        {
          "motionPermission": "Allow Napstr to access your device motion for interactive UI effects"
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e2357696-4ad1-4e72-be0f-439c7e509415"
      }
    }
  }
}
